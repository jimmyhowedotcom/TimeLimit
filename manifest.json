{
    "manifest_version": 2,
    "name": "JHDC Time Limit",
    "description": "Limits the amount of time spent on websites.",
    "version": "0.1",
    "author": "Jimmy Howe",
    "browser_action": {
        "default_icon": "tab-icon.png",
        "default_title": "Have a good day"
    },
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "permissions": [
        "activeTab"
    ]
}